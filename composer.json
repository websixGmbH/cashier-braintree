{
    "abandoned": "braintree/braintree_php",
    "name": "laravel/cashier-braintree",
    "description": "Laravel Cashier provides an expressive, fluent interface to Braintree's subscription billing services.",
    "keywords": ["laravel", "braintree", "billing", "cashier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3.0",
        "braintree/braintree_php": "~3.38",
        "dompdf/dompdf": "^0.8.0",
        "illuminate/contracts": "~6.0",
        "illuminate/database": "~6.0",
        "illuminate/http": "~6.0",
        "illuminate/routing": "~6.0",
        "illuminate/support": "~6.0",
        "illuminate/view": "~6.0",
        "nesbot/carbon": "^1.26.3|^2.0",
        "symfony/http-kernel": "~4.0"
    },
    "require-dev": {
        "ext-json": "*",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
